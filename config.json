{
  "output_mode": "console",
  "output_file": "/var/log/secu-beat.log",
  "use_colors": true,
  "json_output": false,
  "server_url": null,
  "server_token": null,
  "verify_ssl": true,
  "batch_size": 10,
  "timeout": 30,
  "retry_attempts": 3,
  "retry_delay": 5,
  "webhook_url": null,
  "webhook_secret": null,
